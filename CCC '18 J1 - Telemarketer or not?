import java.util.*;

public class Main {
	public static void main(String[] args) {
		
		Scanner n = new Scanner(System.in);
		
		int num1 = n.nextInt();
		int num2 = n.nextInt();
		int num3 = n.nextInt();
		int num4 = n.nextInt();
		
		if((num1 == 8 || num1 == 9) && (num2 == num3) && (num4 == 8 || num4 == 9)) {
			System.out.println("ignore");
		} else {
			System.out.println("answer");
		}
		
	}
}
